    "seqs = []\n",
    "f = open('rosalind_sseq.txt','r').read().split('>')[1:]\n",
    "for entry in f:\n",
    "    seqs.append(''.join(entry.split('\\n')[1:]))\n",
    "ind = [0]\n",
    "for base in seqs[1]:\n",
    "    ind.append(seqs[0][ind[-1]:].index(base)+ind[-1]+1)\n",
    "ind.pop(0)\n",
    "print(*ind)"
   ]
  }

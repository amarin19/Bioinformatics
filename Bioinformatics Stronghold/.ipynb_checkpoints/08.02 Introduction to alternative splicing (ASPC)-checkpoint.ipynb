   "source": [
    "A = list(map(int, open('rosalind_aspc.txt', 'r').read().strip().split(' ')))\n",
    "n, m = A[0], A[1]\n",
    "\n",
    "from math import factorial\n",
    "\n",
    "val=0\n",
    "for k in range (m, n+1): \n",
    "    val += factorial(n)//(factorial(k)*factorial(n-k))\n",
    "print(val%1000000)"
   ]
